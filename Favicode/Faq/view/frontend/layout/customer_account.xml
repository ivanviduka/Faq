<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">

    <head>
        <css src="Favicode_Faq::css/customer-questions-display.css" rel="stylesheet" type="text/css"  />
    </head>

    <body>
        <referenceBlock name="customer_account_navigation">
            <block class="Magento\Framework\View\Element\Html\Link\Current" name="customer-account-questions-list">
                <arguments>
                    <argument name="path" xsi:type="string">faq/customer/myquestionslist</argument>
                    <argument name="label" xsi:type="string">My Product Questions</argument>
                </arguments>
            </block>
        </referenceBlock>
    </body>
</page>
